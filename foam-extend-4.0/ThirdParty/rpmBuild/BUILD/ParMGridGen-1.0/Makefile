default:
	(mkdir bin)
	(cd MGridGen ; make)

serial:
	(mkdir bin)
	(cd MGridGen ; make)

parallel:
	(mkdir bin)
	(cd MGridGen ; make)
	(cd ParMGridGen ; make)
clean:
	(mkdir bin)
	(cd MGridGen ; make clean)
	(cd ParMGridGen ; make clean )

realclean:
	(mkdir bin)
	(cd MGridGen ; make realclean )
	(cd ParMGridGen ; make realclean )
